{
  "name": "package-control",
  "version": "0.12.7",
  "description": "Manage packages from the command palette",
  "private": false,
  "main": "./lib/index",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-pattern .gitignore --no-warn-ignored",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run build",
    "start": "npm run dev",
    "test": "npm run lint",
    "prepare": "husky"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-package-control"
  },
  "homepage": "https://atom.io/packages/package-control",
  "bugs": {
    "url": "https://github.com/idleberg/atom-package-control/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.56.0 <2.0.0",
		"pulsar": ">=1.102.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "package-control:disable-package",
      "package-control:discover-packages",
      "package-control:discover-themes",
      "package-control:enable-package",
      "package-control:install-package",
      "package-control:install-starred-package",
      "package-control:install-all-starred-packages",
      "package-control:list-packages",
      "package-control:remove-package",
      "package-control:satisfy-dependencies",
      "package-control:update-all-packages",
      "package-control:update-package"
    ]
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "browse": {
      "versions": {
        "2.0.0": "consumeBrowse"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    },
    "service-fetch": {
      "versions": {
        "0.2.0": "consumeFetch"
      }
    }
  },
  "package-deps": [
    {
      "name": "browse",
      "minimumVersion": "4.0.0"
    },
    {
      "name": "busy-signal"
    },
    {
      "name": "service-fetch",
      "minimumVersion": "0.2.0"
    }
  ],
  "dependencies": {
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.6.1",
    "@stylelint/remark-preset": "^5.1.1",
    "@types/atom": "^1.40.18",
    "@types/node": "^14.18.63",
    "approximate-number": "^2.1.1",
    "atom-package-deps": "^8.0.0",
    "atom-select-list": "^0.8.1",
    "core-js": "^3.48.0",
    "cross-fetch": "^3.2.0",
    "date-fns": "^2.30.0",
    "encoding": "^0.1.13",
    "eslint": "^9.39.2",
    "eslint-plugin-jsonc": "^2.21.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.1.7",
    "idb-keyval": "^6.2.2",
    "lint-staged": "^15.5.2",
    "log-symbols": "^5.1.0",
    "npm-run-all2": "^6.2.6",
    "open": "^8.4.2",
    "pako": "^2.1.0",
    "prettier": "^3.8.1",
    "remark-cli": "^12.0.1",
    "stylelint": "^13.13.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix",
    "*.{md,yml}": "prettier --write"
  }
}

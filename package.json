{
  "name": "package-control",
  "version": "0.12.1",
  "description": "Manage Atom packages from the command palette",
  "private": false,
  "main": "./lib/index",
  "scripts": {
    "analyze": "source-map-explorer lib/**/*.js",
    "build": "npm run clean && rollup --config",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "format": "prettierx --write ./src",
    "lint:code": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern ./src/**/*.ts",
    "lint:styles": "echo \"Nothing to lint\"",
    "lint": "npm-run-all --parallel lint:*",
    "prepublishOnly": "npm run build",
    "start": "rollup --config --watch",
    "test": "npm run lint"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-package-control"
  },
  "homepage": "https://atom.io/packages/package-control",
  "bugs": {
    "url": "https://github.com/idleberg/atom-package-control/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.52.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "package-control:disable-package",
      "package-control:discover-packages",
      "package-control:discover-themes",
      "package-control:enable-package",
      "package-control:install-package",
      "package-control:install-starred-package",
      "package-control:install-all-starred-packages",
      "package-control:list-packages",
      "package-control:remove-package",
      "package-control:satisfy-dependencies",
      "package-control:update-all-packages",
      "package-control:update-package"
    ]
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "browse": {
      "versions": {
        "1.0.0": "consumeBrowse"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    },
    "service-fetch": {
      "versions": {
        "0.2.0": "consumeFetch"
      }
    }
  },
  "package-deps": [
    {
      "name": "browse",
      "minimumVersion": "3.3.0"
    },
    {
      "name": "busy-signal"
    },
    {
      "name": "service-fetch",
      "minimumVersion": "0.2.0"
    }
  ],
  "dependencies": {
    "approximate-number": "^2.1.0"
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.2",
    "@types/atom": "^1.40.11",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "atom-package-deps": "^7.2.3",
    "atom-select-list": "^0.8.0",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.21.3",
    "encoding": "^0.1.13",
    "eslint": "^7.26.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettierx": "^0.18.0",
    "husky": ">=4.3 <5",
    "idb-keyval": "^5.0.5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^11.0.0",
    "log-symbols": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^8.0.9",
    "pako": "^2.0.3",
    "prettierx": "^0.19.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.1",
    "rollup-plugin-atomic": "^2.3.2",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.json": "jsonlint --quiet",
    "*.{md,yml}": "prettierx --write"
  }
}
